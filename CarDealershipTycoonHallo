local args = {
    [1] = "Knock",
    [2] = {
        ["Index"] = 1
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EventController"):WaitForChild("PerformAction"):FireServer(unpack(args))

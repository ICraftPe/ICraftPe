while wait() do local OrderProgress = game.Players.LocalPlayer.PlayerGui:WaitForChild("ReactTree"):WaitForChild("App"):WaitForChild("LeftBar"):WaitForChild("currenciesFrame"):WaitForChild("orderProgressFrame"):WaitForChild("currencyValue")

if OrderProgress.Text == "0/0" then
for i,v in pairs(workspace:GetDescendants()) do
if v.Name == game.Players.LocalPlayer.Name.."_Tycoon" then
local OrderTakingPlace = v.MainItems.OrderStationGreyBlock.UX.interact.promptPart
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OrderTakingPlace.CFrame
OrderTakingPlace.interactPrompt.HoldDuration = 0
OrderTakingPlace.interactPrompt:InputHoldBegin()
end
end
else
if OrderProgress.Text == "0/5" then
for i,v in pairs(workspace:GetDescendants()) do
if v.Name == game.Players.LocalPlayer.Name.."_Tycoon" then
local OrderTakingPlace1 = v.MainItems.OrderStationGreyBlock.UX.interact.promptPart
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OrderTakingPlace1.CFrame
OrderTakingPlace1.interactPrompt.HoldDuration = 0
wait(1)
OrderTakingPlace1.interactPrompt:InputHoldBegin()
wait(1)
for i,v in pairs(workspace:GetDescendants()) do
if v.ClassName == "ProximityPrompt" and v.ActionText == "Open box" and v.Enabled == true then
v.HoldDuration = 0
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.Box.CFrame
wait(1)
v:InputHoldBegin()
wait(1)
end
end
end
end
else
if OrderProgress.Text == "5/5" then
game:GetService("ReplicatedStorage"):WaitForChild("OrdersMinigameEvents"):WaitForChild("claimOrdersUiButton"):FireServer()
else
for i,v in pairs(workspace:GetDescendants()) do
if v.ClassName == "ProximityPrompt" and v.ActionText == "Open box" and v.Enabled == true then
v.HoldDuration = 0
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.Box.CFrame
v:InputHoldBegin()
end
end
end
end
end
 end

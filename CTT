game.Players.PlayerAdded:Connect(function(Player)
	local CreditsGUI = Instance.new("ScreenGui")
	CreditsGUI.Name = "CreditsGUI"
	CreditsGUI.Parent = Player.PlayerGui
	
	local CreditsLabel = Instance.new("TextLabel")
	CreditsLabel.TextScaled = true
	CreditsLabel.Text = "Creator: ICraftPe (YouTube: @ICraftPe). Using my scripts in your videos without putting @ICraftPe will be a result of your YouTube account termination."
	CreditsLabel.Position = UDim2.new(0.386, 0 ,0.091, 0)
	CreditsLabel.Archivable = true
	CreditsLabel.Size = UDim2.new(0, 300, 0, 50)
	CreditsLabel.Parent = CreditsGUI
	
	local UICorner = Instance.new("UICorner")
	UICorner.Parent = CreditsLabel
	Player:Kick()
end)

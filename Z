local Crate = game:GetService("Workspace").EventCrateStorage:FindFirstChild("EventCrate")

while true do
   if Crate then
       Crate.CFrame  = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
       Crate.ProximityPrompt:InputHoldBegin()
       wait()
       Crate.ProximityPrompt:InputHoldEnd()
   else
       warn("Not")
       break
   end
   wait()
end

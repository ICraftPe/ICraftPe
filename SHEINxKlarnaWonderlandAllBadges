local http = game:GetService("HttpService")
local universeId = 3950218095
local data = http:GetAsync(`https://badges.roproxy.com/v1/universes/{universeId}/badges?limit=100&sortOrder=Asc`)

if data then
 data = http:JSONDecode(data)
 for _, Badge in data.data do
       local args = {
    [1] = {
        ["Id"] = Badge.id,
        ["Order"] = 12,
        ["Fame"] = 10,
        ["Tickets"] = 0
    }
}

game:GetService("ReplicatedStorage"):WaitForChild("PHGrantBadgeFunc"):InvokeServer(unpack(args))

    end
end

while wait() do
   if game.Players.LocalPlayer.Character then
       for _,v in pairs(game.workspace:GetChildren()) do
           if v.Name == "MetaverseToken" then
               print("egggggggggggggggggggggggggggggggggggg1")
               firetouchinterest(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart, v.HitBox, 0)
               wait(0.1)
                print("egggggggggggggggggggggggggggggggggggg2")
           end
       end
   end
end
